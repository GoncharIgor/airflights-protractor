{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "exclude": [
    "target"
  ],
  "member-access": false,
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [ false ],
    "no-var-requires": false,
    "no-console": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "rulesDirectory": []
}
